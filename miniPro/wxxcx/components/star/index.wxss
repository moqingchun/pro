/* components/star/index.wxss */
@import '/assets/iconfont/iconfont.wxss';
.star_wrap{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.star_title{
    color: #333333;
    font-weight: bold;
    font-size: 28rpx;
}
.star_cont{
    display: flex;
}
.star_cont .iconfont+.iconfont{
    margin-left: 18rpx;
}
.star_cont .iconfont{
    color:rgb(220,222,224);
    font-size:40rpx;
}
.star_cont .iconfont.active{
    color: rgb(255, 172, 0);
}
.star_res{
    width: 80rpx;
    color: #999999;
    font-size: 24rpx;
    text-align: right;
}